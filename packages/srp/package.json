    "@semantic-release/npm": "^9.0.0",
    "@semantic-release/release-notes-generator": "^10.0.0",
    "@semantic-release-plus/core": "alpha",
    "aggregate-error": "3.0.0",
    "cosmiconfig": "^7.1.0",
    "debug": "4.3.3",
    "delay": "5.0.0",
    "dockerode": "3.3.1",
    "env-ci": "^5.0.0",
    "execa": "5.1.1",
    "figures": "^3.0.0",
    "file-url": "3.0.0",
    "find-versions": "^4.0.0",
