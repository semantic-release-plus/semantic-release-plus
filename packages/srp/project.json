{
  "root": "packages/srp",
  "sourceRoot": "packages/srp/src",
  "projectType": "library",
  "targets": {
    "pre-build": {
      "executor": "@nrwl/node:package",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/srp",
        "tsConfig": "packages/srp/tsconfig.lib.json",
        "packageJson": "packages/srp/package.json",
        "main": "packages/srp/src/index.js",
        "assets": ["packages/srp/*.md"]
      }
    },
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "chmod +x dist/packages/srp/src/bin/semantic-release-plus.js"
        ]
      },
      "dependsOn": [
        {
          "target": "pre-build",
          "projects": "self"
        }
      ]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/srp/**/*.js"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/srp"],
      "options": {
        "jestConfig": "packages/srp/jest.config.js",
        "passWithNoTests": true
      }
    },
    "ava": {
      "executor": "@nrwl/workspace:run-script",
      "options": {
        "script": "test"
      }
    },
    "e2e": {
      "executor": "@nrwl/workspace:run-script",
      "options": {
        "script": "e2e"
      },
      "dependsOn": [
        {
          "target": "build",
          "projects": "self"
        }
      ]
    },
    "release": {
      "executor": "@nrwl/workspace:run-script",
      "options": {
        "script": "release"
      }
    }
  },
  "tags": []
}
