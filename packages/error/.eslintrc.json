{
  "extends": ["../../.eslintrc.json"],
  "ignorePatterns": ["!**/*", "release.config.js"],
  "overrides": [
    {
      "files": ["*.spec.{js,ts}"],
      "env": {
        "jest": true
      }
    },
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {}
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["*.json"],
      "parser": "jsonc-eslint-parser",
      "rules": {
        "@nx/dependency-checks": "error"
      }
    }
  ]
}
